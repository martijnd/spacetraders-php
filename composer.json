{
    "name": "martijnd/spacetraders-php",
    "type": "library",
    "require": {
        "guzzlehttp/guzzle": "^7.4",
        "illuminate/support": "^8.72"
    },
    "require-dev": {
        "pestphp/pest": "^1.21",
        "phpstan/phpstan": "^1.2"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Martijn Dorsman",
            "email": "martijn.dorsman@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "SpaceTraders\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "phpstan": "vendor/bin/phpstan"
    },
    "minimum-stability": "stable"
}
